# Google Artifact Registry Configuration
# Copy this file to .env.gar and update with your values
# Usage: source .env.gar && ./push-to-gar.sh

# Your GCP Project ID (OPTIONAL - will be auto-extracted from service account)
# export GCP_PROJECT_ID="your-gcp-project-id"

# GAR Region (default: asia-south1 - Mumbai)
# Other options: us-central1, us-east1, europe-west1, asia-east1, etc.
export GAR_REGION="asia-south1"

# GAR Repository name (default: bifrostlink)
export GAR_REPOSITORY="bifrostlink"

# Image version/tag (default: latest)
export IMAGE_VERSION="v1.0.0"

# Service account key file path (default: ./serviceaccount.json)
export SERVICE_ACCOUNT_KEY="./serviceaccount.json"
